
#include<string.h>
#include<stdio.h>
#include<math.h>
#include<iostream>
#include <stdlib.h>
#include <windows.h>
using namespace std;


int main()
{
	float i;
	
	for (i = 1; i < 10086; i++)
	{
		
		if ((i-4-(i/6+i/12+i/7+5))==(i/2))
		{
			printf("%d\n", (int)i - 4);
			break;
		}
	}
	
	return 0;
}

