
#include<string.h>
#include<stdio.h>
#include<math.h>
#include<iostream>
#include <stdlib.h>
#include <windows.h>
using namespace std;


int main()
{
	int i,j=0,n,ok=1;
	scanf("%d", &n);
	for (i = 2; i < n; i++)
	{
		if (n%i == 0)ok = 0;
	}
	if (ok)printf("yes\n");
	else printf("no\n");
	return 0;
}

